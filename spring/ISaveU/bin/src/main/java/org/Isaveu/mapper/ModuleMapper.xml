<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.Isaveu.mapper.ModuleMapper">

<select id="getModuleList" resultType="org.Isaveu.domain.TbModuleVO"
parameterType="String">
	SELECT
		*
	FROM
		tb_module
	WHERE type = #{type}

</select>

</mapper>